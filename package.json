{
  "name": "orchestra",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "dev": "react-router dev",
    "start": "react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc",
    "preinstall": "typesync || :",
    "prisma:all": "npm run prisma:pull && npm run prisma:gen",
    "prisma:pull": "cp templates/schema.prisma prisma/schema.prisma && prisma db pull --schema=prisma/schema.prisma",
    "prisma:gen": "prisma generate --schema=prisma/schema.prisma"
  },
  "dependencies": {
    "@milkdown/crepe": "^7.10.4",
    "@milkdown/kit": "^7.10.4",
    "@milkdown/react": "^7.10.4",
    "@prisma/client": "^6.8.1",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "dompurify": "^3.2.5",
    "inversify": "^7.5.1",
    "isbot": "^5.1.27",
    "jotai": "^2.12.4",
    "jsdom": "^26.1.0",
    "mime": "^4.0.7",
    "quill": "^2.0.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.1",
    "sonner": "^2.0.3",
    "ulid": "^3.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.26.0",
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/jsdom": "~21.1.7",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "eslint": "^9.26.0",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.1.0",
    "prettier": "^3.5.3",
    "prisma": "^6.8.1",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "typesync": "^0.14.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
